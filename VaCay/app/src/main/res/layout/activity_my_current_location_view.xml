<?xml version="1.0" encoding="utf-8"?>
<com.mv.vacay.classes.MapWrapperLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/map_relative_layout"
    android:orientation="vertical"
    tools:context=".main.MyCurrentLocationViewActivity">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <fragment xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:map="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
        <ImageView
            android:id="@+id/imv_profile"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/back2"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="15dp"
            />
        <ImageView
            android:id="@+id/showInfo"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/show2"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="15dp"
            android:background="@drawable/green_fillrect"
            />
    </FrameLayout>


</com.mv.vacay.classes.MapWrapperLayout>